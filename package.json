{
	"name": "dingir",
	"author": "Brent Oates",
	"repository": {
		"type": "git",
		"url": "https://github.com/BrentOates/dingir.git"
	},
	"version": "2.2.4",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "tsc",
		"postbuild": "copyfiles -u 1 -e \"**/*.ts\" \"src/**/*\" dist/src/",
		"lint": "eslint . --ext .ts",
		"dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"npm run copy-and-start\"",
		"copy-and-start": "npm run postbuild && npm start",
		"start": "node dist/src/index.js"
	},
	"dependencies": {
		"@discordjs/rest": "^1.5.0",
		"@napi-rs/canvas": "^0.1.37",
		"discord.js": "^14.7.1",
		"dotenv": "^16.0.3",
		"luxon": "^3.2.1",
		"node-schedule": "^2.1.1",
		"reflect-metadata": "^0.1.13",
		"sequelize": "^6.29.0",
		"sequelize-typescript": "^2.1.5",
		"sqlite3": "^5.1.4",
		"tslib": "^2.5.0",
		"underscore": "^1.13.6"
	},
	"devDependencies": {
		"@types/glob": "^8.1.0",
		"@types/luxon": "^3.2.0",
		"@types/node": "^18.14.2",
		"@types/node-schedule": "^2.1.0",
		"@types/underscore": "^1.11.4",
		"@types/validator": "^13.7.12",
		"@typescript-eslint/eslint-plugin": "^5.53.0",
		"@typescript-eslint/parser": "^5.53.0",
		"copyfiles": "^2.4.1",
		"eslint": "^8.35.0",
		"rimraf": "^4.1.2",
		"tsc-watch": "^6.0.0",
		"typescript": "^4.9.5"
	}
}
