{
	"name": "dingir",
	"author": "Brent Oates",
	"repository": {
		"type": "git",
		"url": "https://github.com/BrentOates/dingir.git"
	},
	"version": "2.5.2",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "tsc",
		"postbuild": "copyfiles -u 1 -e \"**/*.ts\" \"src/**/*\" dist/src/",
		"lint": "eslint . --ext .ts",
		"dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"npm run copy-and-start\"",
		"copy-and-start": "npm run postbuild && npm start",
		"start": "node dist/src/index.js"
	},
	"dependencies": {
		"@discordjs/rest": "^2.0.1",
		"@napi-rs/canvas": "^0.1.44",
		"discord.js": "^14.13.0",
		"dotenv": "^16.3.1",
		"luxon": "^3.4.3",
		"node-schedule": "^2.1.1",
		"reflect-metadata": "^0.1.13",
		"sequelize": "^6.33.0",
		"sequelize-typescript": "^2.1.5",
		"sqlite3": "^5.1.6",
		"tslib": "^2.6.2",
		"underscore": "^1.13.6"
	},
	"devDependencies": {
		"@types/glob": "^8.1.0",
		"@types/luxon": "^3.3.3",
		"@types/node": "^20.8.9",
		"@types/node-schedule": "^2.1.2",
		"@types/underscore": "^1.11.12",
		"@types/validator": "^13.11.5",
		"@typescript-eslint/eslint-plugin": "^6.9.1",
		"@typescript-eslint/parser": "^6.9.0",
		"copyfiles": "^2.4.1",
		"eslint": "^8.52.0",
		"rimraf": "^5.0.5",
		"tsc-watch": "^6.0.4",
		"typescript": "^5.2.2"
	},
	"overrides": {
		"@mapbox/node-pre-gyp": {
			"make-dir": "4.0.0"
		}
	}
}